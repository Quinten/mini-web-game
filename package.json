{
  "name": "mini-web-game",
  "scripts": {
    "start": "parcel --dist-dir ./public",
    "build": "rm -rf public; parcel build",
    "zip": "npm run build; rm -rf game.zip; cd public; zip -r ../game.zip index.html sw.js; cd ..; stat -f%z game.zip;"
  },
  "source": [
    "src/index.html"
  ],
  "targets": {
    "default": {
      "distDir": "./public",
      "sourceMap": false
    }
  },
  "devDependencies": {
    "@parcel/transformer-inline-string": "^2.12.0",
    "parcel": "^2.12.0",
    "process": "^0.11.10"
  },
  "dependencies": {
    "mini-web-game-components": "^1.0.3"
  }
}

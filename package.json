{
  "name": "pg",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "start": "parcel --dist-dir ./public",
    "build": "rm -rf public;parcel build",
    "zip": "rm -rf game.zip;cd public;zip -r ../game.zip index.html sw.js;cd ..;stat -f%z game.zip;",
    "deploy": "npm run build;git add public;git commit -m \"New build\";git push;git subtree push --prefix public origin gh-pages;npm run zip;"
  },
  "source": ["src/index.html", "src/sw.js"],
  "targets": {
    "default": {
      "distDir": "./public",
      "sourceMap": false
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Quinten/pg.git"
  },
  "author": "Quinten",
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/Quinten/pg/issues"
  },
  "homepage": "https://github.com/Quinten/pg#readme",
  "devDependencies": {
    "@parcel/transformer-inline-string": "^2.0.1",
    "parcel": "^2.0.1"
  }
}
